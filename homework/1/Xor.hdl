/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=or1);
    And(a=a, b=b, out=and1);
    Not(in=and1, out=nand1);
    And(a=or1, b=nand1, out=out);
}